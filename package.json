{
  "name": "super_basic_ts_template",
  "version": "1.0.0",
  "description": "My preferred super basic node.js typescript scripting project setup.",
  "main": "dist/index.ts",
  "author": "Zippy",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn run compile && node dist/index.js",
    "compile": "swc --config-file .swcrc ./src -d dist",
    "pretty": "prettier --config ./.prettierrc.json -w ./src"
  },
  "dependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.121",
    "lodash": "^4.17.21",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.1",
    "prettier": "^2.5.1"
  }
}
